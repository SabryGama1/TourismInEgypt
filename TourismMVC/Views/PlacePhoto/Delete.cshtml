@model TourismMVC.ViewModels.PlacePhotoViewModel

@{
    ViewData["Title"] = "Delete";
}


<div class="m-auto w-75 bg-white p-5">
    <h1>Delete Place Photo</h1>

    <hr />

    <form asp-action="Delete" method="post">
       
        <partial name="PartialViews/DeleteDetialsView" />


        <input type="hidden" asp-for="PlaceId" />

        <input type="hidden" asp-for="Photo" />

        <div class="form-group mt-2 p-5">
            <input type="submit" value="Delete" class="btn btn-danger " />
            <a asp-action="Index" class="btn btn-light">Back to List</a>
        </div>
    </form>

</div>