@model PlaceViewModel


@{
    ViewData["Title"] = "Create";
}


<div class="m-auto w-75 bg-white p-5 ">
<h1>Add New Place</h1>

<hr />

<form asp-controller="Place" asp-action="Create" method="post">


    <partial name="PartialViews/CreateEditView" />

    <div class="form-group mt-4 p-5">
            <button type="submit" class="btn btn-primary">Submit</button>
        <a asp-action="Index" class="btn btn-light">Back to List</a>
    </div>
</form>

</div>
@section Scripts{
    <partial name="_ValidationScriptsPartial" />

}