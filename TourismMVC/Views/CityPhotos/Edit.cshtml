@model CityPhotosViewModel


@{
    ViewData["Title"] = "Edit";
}


<div class="m-auto w-75 bg-white p-5">
    <h1>Edit City Photo</h1>

    <hr />

    <form asp-action="Edit" method="post" enctype="multipart/form-data">

        
        <partial name="PartialViews/CreateEditView" />
        <input type="hidden" asp-for="Photo" />
        
        <div class="form-group mt-3 p-5">
            <input type="submit" value="Save" class="btn btn-primary" />
            <a asp-action="Index" class="btn btn-light">Back to List</a>
        </div>
    </form>

</div>
@section Scripts{
    <partial name="_ValidationScriptsPartial" />

}